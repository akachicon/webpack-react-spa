{
  "name": "webpack-babel-ts-client-app-config",
  "version": "1.0.0",
  "description": "webpack/babel/ts client app config",
  "license": "ISC",
  "author": "Zharenko Denis",
  "browserslist": {
    "production": [
      "> 0.5%",
      "last 2 versions",
      "firefox ESR",
      "not dead",
      "not ie > 0",
      "not ie_mob > 0",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome versions",
      "last 3 firefox versions"
    ]
  },
  "scripts": {
    "build:base": "webpack --config ./webpack-config/webpack.base.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack-config/webpack.dev.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack-config/webpack.prod.config.js",
    "serve:dev-server": "cross-env NODE_ENV=development CSS_MODE=hot webpack-dev-server --config ./webpack-config/webpack.dev.config.js",
    "lint:css": "stylelint --cache --cacheLocation=./node_modules/.cache/stylelint/ ./src",
    "clear:cache": "rm -rf ./node_modules/.cache"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.9.4",
    "@pmmmwh/react-refresh-webpack-plugin": "github:pmmmwh/react-refresh-webpack-plugin",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "favicons": "^6.0.0",
    "favicons-webpack-plugin": "^3.0.1",
    "fibers": "^5.0.0",
    "file-loader": "github:akachicon/file-loader",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "github:akachicon/html-webpack-plugin",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.1.0",
    "imagemin-webp": "^5.1.0",
    "imagemin-webpack": "^5.1.1",
    "mime-types": "^2.1.27",
    "postcss-loader": "^3.0.0",
    "postcss-media-minmax": "^4.0.0",
    "react-refresh": "^0.8.2",
    "regex-escape": "^3.4.9",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-ext-html-webpack-plugin": "^4.1.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-rational-order": "github:akachicon/stylelint-rational-order",
    "stylelint-scss": "^3.17.1",
    "url-loader": "^4.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "core-js": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
